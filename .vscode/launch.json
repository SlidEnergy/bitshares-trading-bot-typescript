	"version": "0.2.0",
	"configurations": [
		{
			"name": "Trade index.ts",
			"type": "node",
			"request": "launch",
            "preLaunchTask": "tsc",
			"program": "${workspaceRoot}/index.ts",
			"args": ["-debug", "-trade"],
			"outFiles": ["${workspaceRoot}/bin/*.js"]
		},
		{
			"name": "Debug index.ts",
			"type": "node",
			"request": "launch",
            "preLaunchTask": "tsc",
			"program": "${workspaceRoot}/index.ts",
			"args": ["-debug"],
			"outFiles": ["${workspaceRoot}/bin/*.js"]
		}
	]
}